<link rel="stylesheet" href="/styles/styles-index.css" media="screen" title="no title" charset="utf-8">

<div id="browse">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h3>Art Posting:</h3>
        <ul id="artpostList" class="list-group">
          {{#each artposts}}
          <li data-id="{{this.id}}" class="list-group-item">
            <img src={{this.URL}}>
            <br>
            <a href="artpost/{{this.id}}">{{this.title}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/browse.js"></script>