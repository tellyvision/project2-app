<link rel="stylesheet" href="/styles/styles-browse.css" media="screen" title="no title" charset="utf-8">

<div id="browse">
  <div class="container-fluid">
    <div class="row">
      <div class="col">
        <h3>Browse</h3>
      </div>
    </div>
    <div class="row no-gutters" id="artpostList">
      {{#each artposts}}
      <div class="col-lg-3 animated fadeIn">
        <div class="card mt-4" data-id="{{this.id}}">
          <a href="artpost/{{this.id}}">
            <img class="artimg" src={{this.URL}}>
          </a>
          <div class="card-body">
            <a href="artpost/{{this.id}}">
              {{this.title}} by {{this.artist}}
            </a>
            <button class="btn btn-sm btn-danger float-right delete">ï½˜</button>
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/browse.js"></script>